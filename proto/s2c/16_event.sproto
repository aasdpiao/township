#通知客户端 事件更新需要发送request_event消息
event_update 1601{
	request {
		timestamp 0 : integer
	}
	response {
		result 0 : integer
	}
}


unlock_main_task 1602{
	request {
		task_index 0 : integer
		task_objects 0 : *task_object
	}
	response {
		result 0 : integer
	}
}

finish_main_task 1603{
	request {
		task_index 0 : integer
	}
	response {
		result 0 : integer
	}
}

