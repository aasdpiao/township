	request {
		timestamp 0 : integer
		cash_count 1 : integer
	}
	response {
		result 0 : integer
	}
}

finish_flight 903{
	request {
		timestamp 0 : integer
		item_objects 1 : *item_object
	}
	response {
		result 0 : integer
		flight_orders 1 : *flight_order
		flight_rewards 2 : *flight_reward
		timestamp 3 : integer
		station_status 4 : integer
	}
}

